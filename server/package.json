  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "test": "ts-jest",
    "start-dev": "nodemon --watch .env.local --exec \"npm run gen-env-types && npm run env-inject\"",
    "gen-env-types": "gen-env-types -e . -o ./src/env.d.ts .env.local",
    "env-inject": "dotenvx run --env-file=.env.local -- concurrently \"npm run monitor-files\" \"pgtyped -w -c ./pgtyped-config.cjs\"",
    "monitor-files": "nodemon --watch ./src --ext ts --exec \"node --inspect -r ts-node/register -r tsconfig-paths/register ./src/index.ts\""
  },
  "keywords": [],
  "author": "",
