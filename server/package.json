    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.14.0",
    "gen-env-types": "^1.3.4",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
